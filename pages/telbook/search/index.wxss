@import '../../../utils/iconfont.wxss';
page{
  background: #EFEFF4;
}
.phone{
  width: 100%;
  height: 150rpx;
  background: #fff;
  margin: 20rpx 0;
  overflow: hidden;
  position: relative;
}
.phone-name{
  position: absolute;
  font-size: 36rpx;
  top: 35rpx;
  left:30rpx;
  line-height: 40rpx;
}
.phone-content{
  position: absolute;
  font-size: 30rpx;
  color: #787878;
  top: 85rpx;
  left:30rpx; 
}
.phone-icon{
  width: 90rpx;
  height: 90rpx;
  position: absolute;
  right: 50rpx;
  top: 0;
  bottom: 0;
  margin: auto;
  text-align: center;
  line-height: 90rpx;
  border-radius: 50rpx;
  background: #FED636;
}
.phone-icon .iconfont{
  font-size: 56rpx;
  color:white;
}
.Loading-bottom{
  text-align: center;
  margin: 10rpx 0;
}
.container1{
  width: 100%;
  height: 945rpx;
}
.vip{
  width: 40rpx;
  height: 40rpx;
  float: left;
  margin-right: 10rpx;
}
.vip image{
  width: 100%;
  height: 100%;
}